# Main Application
NODE_ENV=development
PORT=3000
API_KEY=your-api-key-here
LOG_LEVEL=info

# Database Configuration
DATABASE_URL=postgresql://analytics_user:analytics_password@localhost:5432/analytics_db
REDIS_URL=redis://localhost:6379

# AWS Configuration
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
ATHENA_WORKGROUP=primary
ATHENA_OUTPUT_LOCATION=s3://your-athena-results-bucket/

# LLM Configuration
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
LLM_PROVIDER=openai
LLM_MODEL=gpt-4

# Email Configuration
EMAIL_PROVIDER=sendgrid
SENDGRID_API_KEY=your-sendgrid-api-key
FROM_EMAIL=analytics@yourcompany.com
FROM_NAME=Analytics Agent

# File Storage
DATA_DIR=./data
TEMP_DIR=./data/temp
CACHE_DIR=./data/cache
EXPORTS_DIR=./data/exports
LOGS_DIR=./logs

# External Access
NGROK_AUTH_TOKEN=your-ngrok-auth-token
EXTERNAL_URL=https://your-domain.ngrok.io

# Security
JWT_SECRET=your-jwt-secret-key
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100